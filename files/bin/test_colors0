#!/usr/bin/bash

for i in {0..7}; do
    printf -v "df$i" %b "\e[2;3${i}m"
    printf -v "f$i" %b "\e[3${i}m"
    printf -v "bf$i" %b "\e[9${i}m"
done

rst=$'\e[0m'

cat << EOF
 $df1▄ ▀▄   ▄▀ ▄   $df2 ▄▄▄████▄▄▄    $df3  ▄██▄     $df4▄ ▀▄   ▄▀ ▄   $df5 ▄▄▄████▄▄▄    $df6  ▄██▄  $rst
 $df1█▄█▀███▀█▄█   $df2███▀▀██▀▀███   $df3▄█▀██▀█▄   $df4█▄█▀███▀█▄█   $df5███▀▀██▀▀███   $df6▄█▀██▀█▄$rst
 $df1▀█████████▀   $df2▀▀▀██▀▀██▀▀▀   $df3▀▀█▀▀█▀▀   $df4▀█████████▀   $df5▀▀▀██▀▀██▀▀▀   $df6▀▀█▀▀█▀▀$rst
 $df1 ▄▀     ▀▄    $df2▄▄▀▀ ▀▀ ▀▀▄▄   $df3▄▀▄▀▀▄▀▄   $df4 ▄▀     ▀▄    $df5▄▄▀▀ ▀▀ ▀▀▄▄   $df6▄▀▄▀▀▄▀▄$rst

 $f1  ▀▄   ▄▀     $f2 ▄▄▄████▄▄▄    $f3  ▄██▄     $f4  ▀▄   ▄▀     $f5 ▄▄▄████▄▄▄    $f6  ▄██▄  $rst
 $f1 ▄█▀███▀█▄    $f2███▀▀██▀▀███   $f3▄█▀██▀█▄   $f4 ▄█▀███▀█▄    $f5███▀▀██▀▀███   $f6▄█▀██▀█▄$rst
 $f1█▀███████▀█   $f2▀▀███▀▀███▀▀   $f3▀█▀██▀█▀   $f4█▀███████▀█   $f5▀▀███▀▀███▀▀   $f6▀█▀██▀█▀$rst
 $f1▀ ▀▄▄ ▄▄▀ ▀   $f2 ▀█▄ ▀▀ ▄█▀    $f3▀▄    ▄▀   $f4▀ ▀▄▄ ▄▄▀ ▀   $f5 ▀█▄ ▀▀ ▄█▀    $f6▀▄    ▄▀$rst

 $bf1▄ ▀▄   ▄▀ ▄   $bf2 ▄▄▄████▄▄▄    $bf3  ▄██▄     $bf4▄ ▀▄   ▄▀ ▄   $bf5 ▄▄▄████▄▄▄    $bf6  ▄██▄  $rst
 $bf1█▄█▀███▀█▄█   $bf2███▀▀██▀▀███   $bf3▄█▀██▀█▄   $bf4█▄█▀███▀█▄█   $bf5███▀▀██▀▀███   $bf6▄█▀██▀█▄$rst
 $bf1▀█████████▀   $bf2▀▀▀██▀▀██▀▀▀   $bf3▀▀█▀▀█▀▀   $bf4▀█████████▀   $bf5▀▀▀██▀▀██▀▀▀   $bf6▀▀█▀▀█▀▀$rst
 $bf1 ▄▀     ▀▄    $bf2▄▄▀▀ ▀▀ ▀▀▄▄   $bf3▄▀▄▀▀▄▀▄   $bf4 ▄▀     ▀▄    $bf5▄▄▀▀ ▀▀ ▀▀▄▄   $bf6▄▀▄▀▀▄▀▄$rst


                                     $bf7▌$rst

                                   $bf7▌$rst

                              $f7    ▄█▄    $rst
                              $f7▄█████████▄$rst
                              $f7▀▀▀▀▀▀▀▀▀▀▀$rst
EOF
